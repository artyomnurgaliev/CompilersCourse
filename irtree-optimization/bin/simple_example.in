   if (a > 1) {
      int c;
      c = 3;
   }

   if (a == 1) {
      int c;
      c = 3;
   } else {
      int d;
      d = 4;
   }

   while (a < 10) {
     a = a + 1;
   }
  }
}

class Array {
    int[] a;
    int[] b;
    public void ChangeSize(int size) {
        a = new int[size];
        b = new int[size * 2];
        a[1] = 2;
        b[2] = 3;
    }
}

class Conditions {
    int a;
    int b;

    public void CheckIF() {
        a = 2;
        if (a > 1) {
            a = 3;
        }
    }
    public void CheckIFELSE() {
            a = 2;
            if (a > 1) {
                a = 3;
            } else {
                a = 4;
            }
    }
    public void CheckWHILE() {
            b = 3;
            while (b > 1) {
                b = b - 1;
            }
    }
}

